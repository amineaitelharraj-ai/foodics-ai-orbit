{
  "name": "@foodics/labeeb-chat",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/labeeb-chat.umd.cjs",
  "module": "./dist/labeeb-chat.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/labeeb-chat.js",
      "require": "./dist/labeeb-chat.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./widget": {
      "import": "./dist/widget.js",
      "require": "./dist/widget.umd.cjs",
      "types": "./dist/widget/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:widget": "vite build --config vite.config.widget.ts",
    "build:all": "pnpm build && pnpm build:widget",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist node_modules"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "lucide-vue-next": "^0.303.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.6.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^2.0.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.1.0"
  }
}
